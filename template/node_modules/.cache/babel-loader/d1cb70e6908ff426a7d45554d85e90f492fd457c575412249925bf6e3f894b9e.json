{"ast":null,"code":"import axios from 'axios';\nimport LayoutDiv from '../LayoutDiv.vue';\nimport Swal from 'sweetalert2';\nexport default {\n  name: 'LivreCreate',\n  components: {\n    LayoutDiv\n  },\n  data() {\n    return {\n      formData: {},\n      project: {},\n      isSaving: false\n    };\n  },\n  created() {\n    // prends la valeur de l'id specifie dans l'url /api/:id\n\n    axios.get(`/Biblio/tocrudlivre.do`).then(response => {\n      let projectInfo = response.data.o[0];\n      this.project.id = projectInfo.id;\n      this.project.label = projectInfo.label;\n      this.project.auteurs = response.data.auteurs;\n      this.project.auteur = projectInfo.auteur;\n      this.project.editeurs = response.data.editeurs;\n      this.project.editeur = projectInfo.editeur;\n      this.project.datePublication = projectInfo.datePublication;\n      let obj = projectInfo;\n      for (let key in obj) {\n        let value = obj[key];\n        if (typeof value === 'object' && value !== null) {\n          obj[key] = obj[key].label;\n        }\n      }\n      return response;\n    }).catch(error => {\n      Swal.fire({\n        icon: 'error',\n        title: 'An Error Occured!',\n        showConfirmButton: false,\n        timer: 1500\n      });\n      return error;\n    });\n  },\n  methods: {\n    handleSave() {\n      this.isSaving = true;\n      const formData = new FormData();\n      formData.append('auteur', this.project.auteur.id);\n      formData.append('editeur', this.project.editeur.id);\n      axios.post(`Biblio/insertlivre.do`, formData).then(response => {\n        Swal.fire({\n          icon: 'success',\n          title: 'Project updated successfully!',\n          showConfirmButton: false,\n          timer: 1500\n        });\n        this.isSaving = false;\n        this.project.id = \"\";\n        this.project.label = \"\";\n        this.project.auteurs = \"\";\n        this.project.auteur = \"\";\n        this.project.editeurs = \"\";\n        this.project.editeur = \"\";\n        this.project.datePublication = \"\";\n        return response;\n      }).catch(error => {\n        this.isSaving = false;\n        Swal.fire({\n          icon: 'error',\n          title: 'An Error Occured!',\n          showConfirmButton: false,\n          timer: 1500\n        });\n        return error;\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","LayoutDiv","Swal","name","components","data","formData","project","isSaving","created","get","then","response","projectInfo","o","id","label","auteurs","auteur","editeurs","editeur","datePublication","obj","key","value","catch","error","fire","icon","title","showConfirmButton","timer","methods","handleSave","FormData","append","post"],"sources":["D:\\GenesisV2_Framework_Psql\\template\\src\\components\\pages\\LivreCreate.vue"],"sourcesContent":["<template>\r\n    <layout-div>\r\n         <h2 class=\"text-center mt-5 mb-3\">Edit Project</h2>\r\n         <div class=\"card\">\r\n             <div class=\"card-header\">\r\n                 <router-link \r\n                     class=\"btn btn-outline-info float-right\"\r\n                     to=\"/\">View All Projects\r\n                 </router-link>\r\n             </div>\r\n             <div class=\"card-body\">\r\n                <form>\r\n                    <div class=\"form-group\">\r\n    <label htmlFor=\"label\">label</label>\r\n    <input \r\n        v-model=\"project.label\"\r\n        type=\"String\"\r\n        class=\"form-control\"\r\n        id=\"label\"\r\n        name=\"label\"/>\r\n</div>\n\t\t<div class=\"form-group\">\r\n    <label htmlFor=\"auteur\">auteur</label>\r\n<select name=\"auteur\" class=\"form-control\" v-model=\"project.auteur.id\">\r\n <option :value=\"project.auteur.id\">{{ project.auteur.label }}</option>\r\n   <option v-for=\"auteur in project.auteurs\" :key=\"auteur.id\" :value=\"auteur.id\">{{ auteur.label }}</option>\r\n</select>\r\n</div>\n\t\t<div class=\"form-group\">\r\n    <label htmlFor=\"editeur\">editeur</label>\r\n<select name=\"editeur\" class=\"form-control\" v-model=\"project.editeur.id\">\r\n <option :value=\"project.editeur.id\">{{ project.editeur.label }}</option>\r\n   <option v-for=\"editeur in project.editeurs\" :key=\"editeur.id\" :value=\"editeur.id\">{{ editeur.label }}</option>\r\n</select>\r\n</div>\n\t\t<div class=\"form-group\">\r\n    <label htmlFor=\"datePublication\">datePublication</label>\r\n    <input \r\n        v-model=\"project.datePublication\"\r\n        type=\"java.time.LocalDate\"\r\n        class=\"form-control\"\r\n        id=\"datePublication\"\r\n        name=\"datePublication\"/>\r\n</div>\n\t\t\r\n                     <button \r\n                         @click=\"handleSave()\"\r\n                         :disabled=\"isSaving\"\r\n                         type=\"button\"\r\n                         class=\"btn btn-outline-primary mt-3\">\r\n                         Save Project\r\n                     </button>\r\n                </form>\r\n             </div>\r\n         </div>\r\n    </layout-div>\r\n </template>\r\n <script>\r\n\nimport  axios from 'axios';\n\nimport   LayoutDiv from '../LayoutDiv.vue';\n\nimport   Swal from 'sweetalert2';\n\r\n export default {\r\n   name: 'LivreCreate',\r\n   components: {\r\n     LayoutDiv,\r\n   },\r\n   data() {\r\n     return {\r\n            formData:{\r\n        \r\n      },\r\n       project: {\r\n         \r\n       },\r\n       isSaving:false,\r\n     };\r\n   },\r\n      created() {\r\n        // prends la valeur de l'id specifie dans l'url /api/:id\r\n     \r\n     \r\n     axios.get(`/Biblio/tocrudlivre.do`)\r\n     .then(response => {\r\n         let projectInfo = response.data.o[0]\r\n         this.project.id = projectInfo.id\n\t\tthis.project.label = projectInfo.label\n\t\tthis.project.auteurs = response.data.auteurs\n\t\tthis.project.auteur = projectInfo.auteur\n\t\tthis.project.editeurs = response.data.editeurs\n\t\tthis.project.editeur = projectInfo.editeur