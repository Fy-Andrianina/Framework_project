{"ast":null,"code":"import axios from 'axios';\nimport LayoutDiv from '../LayoutDiv.vue';\nimport Swal from 'sweetalert2';\nimport { forEach } from 'core-js/core/array';\nexport default {\n  name: 'LivreList',\n  components: {\n    LayoutDiv\n  },\n  data() {\n    return {\n      projects: []\n    };\n  },\n  created() {\n    this.fetchProjectList();\n  },\n  methods: {\n    fetchProjectList() {\n      axios.get('Biblio/tocrudlivre.do').then(response => {\n        this.projects = response.data.o;\n        for (let key in response.data.o) {\n          if (response.data.o.hasOwnProperty(key)) {\n            // Utilisez key pour accéder à chaque attribut de l'objet\n            let value = response.data.o[key];\n            console.log(`Clé: ${key}, Valeur: ${value}`);\n            // Faites quelque chose avec la clé et la valeur\n          }\n        }\n        return response;\n      }).catch(error => {\n        return error;\n      });\n    },\n    handleDelete(id) {\n      Swal.fire({\n        title: 'Are you sure?',\n        text: \"You won't be able to revert this!\",\n        icon: 'warning',\n        showCancelButton: true,\n        confirmButtonColor: '#3085d6',\n        cancelButtonColor: '#d33',\n        confirmButtonText: 'Yes, delete it!'\n      }).then(result => {\n        if (result.isConfirmed) {\n          axios.delete(`/Biblio/deletelivre.do?id=${id}`).then(response => {\n            Swal.fire({\n              icon: 'success',\n              title: 'Project deleted successfully!',\n              showConfirmButton: false,\n              timer: 1500\n            });\n            this.fetchProjectList();\n            return response;\n          }).catch(error => {\n            Swal.fire({\n              icon: 'error',\n              title: 'An Error Occured!',\n              showConfirmButton: false,\n              timer: 1500\n            });\n            return error;\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","LayoutDiv","Swal","forEach","name","components","data","projects","created","fetchProjectList","methods","get","then","response","o","key","hasOwnProperty","value","console","log","catch","error","handleDelete","id","fire","title","text","icon","showCancelButton","confirmButtonColor","cancelButtonColor","confirmButtonText","result","isConfirmed","delete","showConfirmButton","timer"],"sources":["D:\\GenesisV2_Framework_Psql\\template\\src\\components\\pages\\LivreList.vue"],"sourcesContent":["<template>\r\n    <layout-div>\r\n          <div class=\"container\">\r\n              <h2 class=\"text-center mt-5 mb-3\">Project Manager</h2>\r\n              <div class=\"card\">\r\n                  <div class=\"card-header\">\r\n                      <router-link to=\"/create\"\r\n                          class=\"btn btn-outline-primary\"\r\n                          >Create New Project\r\n                      </router-link>\r\n                  </div>\r\n                  <div class=\"card-body\">\r\n               \r\n                      <table class=\"table table-bordered\">\r\n                          <thead>\r\n                              <tr>\r\n                                  <th>id</th>\r\n\t\t<th>label</th>\r\n\t\t<th>auteur</th>\r\n\t\t<th>editeur</th>\r\n\t\t<th>datePublication</th>\r\n\t\t\r\n                                  <th width=\"240px\">Action</th>\r\n                              </tr>\r\n                          </thead>\r\n                          <tbody>\r\n                               \r\n                              <tr v-for=\"project in projects\" :key=\"project.id\">\r\n                                <td>{{project.id}}</td>\r\n\t\t<td>{{project.label}}</td>\r\n\t\t<td>{{project.auteur}}</td>\r\n\t\t<td>{{project.editeur}}</td>\r\n\t\t<td>{{project.datePublication}}</td>\r\n\t\t\r\n                                  <td>\r\n                                  <router-link :to=\"`/LivreEdit/${project.id}`\" class=\"btn btn-outline-info mx-1\">Edit</router-link>\r\n\t\t<router-link :to=\"`/LivreShow/${project.id}`\" class=\"btn btn-outline-info mx-1\">Show</router-link>\r\n\t\t<router-link :to=\"`/LivreCreate`\" class=\"btn btn-outline-info mx-1\">Create</router-link>\r\n\t\t\r\n                                      <button \r\n                                          @click=\"handleDelete(project.id)\"\r\n                                          className=\"btn btn-outline-danger mx-1\">\r\n                                          Delete\r\n                                      </button>\r\n                                  </td>\r\n                              </tr>\r\n                                   \r\n                          </tbody>\r\n                      </table>\r\n                  </div>\r\n              </div>\r\n          </div>\r\n      </layout-div>\r\n  </template>\r\n   <script>\r\n\r\nimport  axios from 'axios';\r\n\r\nimport   LayoutDiv from '../LayoutDiv.vue';\r\n\r\nimport   Swal from 'sweetalert2';\r\nimport { forEach } from 'core-js/core/array';\r\n\r\n   \r\n  export default {\r\n    name: 'LivreList',\r\n    components: {\r\n      LayoutDiv,\r\n    },\r\n    data() {\r\n      return {\r\n        projects:[]\r\n      };\r\n    },\r\n    created() {\r\n      this.fetchProjectList();\r\n    },\r\n    methods: {\r\n      fetchProjectList() {\r\n        axios.get('Biblio/tocrudlivre.do')\r\n          .then(response => {\r\n              this.projects = response.data.o;\r\n              for (let key in response.data.o) {\r\n            if (response.data.o.hasOwnProperty(key)) {\r\n                // Utilisez key pour accéder à chaque attribut de l'objet\r\n                let value = response.data.o[key];\r\n                console.log(`Clé: ${key}, Valeur: ${value}`);\r\n                // Faites quelque chose avec la clé et la valeur\r\n            }\r\n        }\r\n              return response\r\n          })\r\n          .catch(error => {\r\n            return error\r\n          });\r\n      },\r\n      handleDelete(id){\r\n                  Swal.fire({\r\n              title: 'Are you sure?',\r\n              text: \"You won't be able to revert this!\",\r\n              icon: 'warning',\r\n              showCancelButton: true,\r\n              confirmButtonColor: '#3085d6',\r\n              cancelButtonColor: '#d33',\r\n              confirmButtonText: 'Yes, delete it!'\r\n            }).then((result) => {\r\n              if (result.isConfirmed) {\r\n                  axios.delete(`/Biblio/deletelivre.do?id=${id}`)\r\n                  .then( response => {\r\n                      Swal.fire({\r\n                          icon: 'success',\r\n                          title: 'Project deleted successfully!',\r\n                          showConfirmButton: false,\r\n                          timer: 1500\r\n                      })\r\n                      this.fetchProjectList();\r\n                      return response\r\n                  })\r\n                  .catch(error => {\r\n                      Swal.fire({\r\n                           icon: 'error',\r\n                          title: 'An Error Occured!',\r\n                          showConfirmButton: false,\r\n                          timer: 1500\r\n                      })\r\n                      return error\r\n                  });\r\n              }\r\n            })\r\n      }\r\n    },\r\n  };\r\n  </script>"],"mappings":"AAwDA,OAAQA,KAAI,MAAO,OAAO;AAE1B,OAASC,SAAQ,MAAO,kBAAkB;AAE1C,OAASC,IAAG,MAAO,aAAa;AAChC,SAASC,OAAM,QAAS,oBAAoB;AAG1C,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IACVJ;EACF,CAAC;EACDK,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAC;IACX,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,gBAAgB,CAAC,CAAC;EACzB,CAAC;EACDC,OAAO,EAAE;IACPD,gBAAgBA,CAAA,EAAG;MACjBT,KAAK,CAACW,GAAG,CAAC,uBAAuB,EAC9BC,IAAI,CAACC,QAAO,IAAK;QACd,IAAI,CAACN,QAAO,GAAIM,QAAQ,CAACP,IAAI,CAACQ,CAAC;QAC/B,KAAK,IAAIC,GAAE,IAAKF,QAAQ,CAACP,IAAI,CAACQ,CAAC,EAAE;UACnC,I