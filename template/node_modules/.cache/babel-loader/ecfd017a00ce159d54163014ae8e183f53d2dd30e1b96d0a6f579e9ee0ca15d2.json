{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport LayoutDiv from '../LayoutDiv.vue';\nimport Swal from 'sweetalert2';\nimport SideBar from './SideBar';\nexport default {\n  name: 'LivreList',\n  components: {\n    LayoutDiv,\n    SideBar\n  },\n  data() {\n    return {\n      projects: [],\n      currentPage: 1,\n      itemsPerPage: 4,\n      // Nombre d'éléments par page\n      totalPages: 0\n    };\n  },\n  created() {\n    this.fetchProjectList();\n  },\n  methods: {\n    fetchProjectList() {\n      const formData = new FormData();\n      if (sessionStorage.getItem('role') != null) {\n        formData.append('role', sessionStorage.getItem('role'));\n      }\n      const offset = (this.currentPage - 1) * this.itemsPerPage;\n      axios.post(`Biblio/tocrudlivre.do?offset=${offset}`, formData).then(response => {\n        if (Object.keys(response.data).length === 0) {\n          this.$router.push('/');\n        }\n        this.projects = response.data.o;\n        this.totalPages = Math.ceil(response.data.pages / this.itemsPerPage);\n        for (let i = 0; i < this.projects.length; i++) {\n          let obj = this.projects[i];\n          for (let key in obj) {\n            let value = obj[key];\n            if (typeof value === 'object' && value !== null) {\n              obj[key] = obj[key].label;\n            }\n          }\n        }\n        return response;\n      }).catch(error => {\n        return error;\n      });\n    },\n    goToPage(pageNumber) {\n      console.log('goToPage called with pageNumber:', pageNumber);\n      this.currentPage = pageNumber;\n      this.fetchProjectList();\n    },\n    handleDelete(id) {\n      Swal.fire({\n        title: 'Are you sure?',\n        text: \"You won't be able to revert this!\",\n        icon: 'warning',\n        showCancelButton: true,\n        confirmButtonColor: '#3085d6',\n        cancelButtonColor: '#d33',\n        confirmButtonText: 'Yes, delete it!'\n      }).then(result => {\n        if (result.isConfirmed) {\n          const formData = new FormData();\n          if (sessionStorage.getItem('role') != null) {\n            formData.append('role', sessionStorage.getItem('role'));\n          }\n          axios.post(`/Biblio/deletelivre.do?id=${id}`, formData).then(response => {\n            if (Object.keys(response.data).length === 0) {\n              this.$router.push('/');\n            }\n            Swal.fire({\n              icon: 'success',\n              title: 'Project deleted successfully!',\n              showConfirmButton: false,\n              timer: 1500\n            });\n            this.fetchProjectList();\n            return response;\n          }).catch(error => {\n            Swal.fire({\n              icon: 'error',\n              title: 'An Error Occured!',\n              showConfirmButton: false,\n              timer: 1500\n            });\n            return error;\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","LayoutDiv","Swal","SideBar","name","components","data","projects","currentPage","itemsPerPage","totalPages","created","fetchProjectList","methods","formData","FormData","sessionStorage","getItem","append","offset","post","then","response","Object","keys","length","$router","push","o","Math","ceil","pages","i","obj","key","value","label","catch","error","goToPage","pageNumber","console","log","handleDelete","id","fire","title","text","icon","showCancelButton","confirmButtonColor","cancelButtonColor","confirmButtonText","result","isConfirmed","showConfirmButton","timer"],"sources":["D:\\ITU\\S6\\framework\\Framework_project\\template\\src\\components\\pages\\LivreList.vue"],"sourcesContent":["<template>\r\n<div>\r\n    <SideBar/>\r\n    <layout-div>\r\n          <div class=\"container\">\r\n              <h2 class=\"text-center mt-5 mb-3\">FOII template</h2>\r\n              <div class=\"card\">\r\n                  <div class=\"card-header\">\r\n                      <router-link to=\"/LivreCreate\" class=\"btn btn-outline-primary\">Create</router-link>\r\n                  </div>\r\n                  <div class=\"card-body\">\r\n                      <table class=\"table table-bordered\">\r\n                          <thead>\r\n                              <tr>\r\n                                  <th>id</th>\r\n                                  <th>label</th>\r\n                                  <th>auteur</th>\r\n                                  <th>editeur</th>\r\n                                  <th>datePublication</th>\r\n                                  <th width=\"240px\">Action</th>\r\n                              </tr>\r\n                          </thead>\r\n                          <tbody>\r\n                              <tr v-for=\"project in projects\" :key=\"project.id\">\r\n                                  <td>{{project.id}}</td>\r\n                                  <td>{{project.label}}</td>\r\n                                  <td>{{project.auteur}}</td>\r\n                                  <td>{{project.editeur}}</td>\r\n                                  <td>{{project.datePublication}}</td>\r\n                                  <td>\r\n                                      <router-link :to=\"`/LivreEdit/${project.id}`\" class=\"btn btn-outline-info mx-1\">Edit</router-link>\r\n                                      <router-link :to=\"`/LivreShow/${project.id}`\" class=\"btn btn-outline-info mx-1\">Show</router-link>\r\n                                      <router-link :to=\"`/LivreCreate`\" class=\"btn btn-outline-info mx-1\">Create</router-link>\r\n                                      <button @click=\"handleDelete(project.id)\" class=\"btn btn-outline-danger mx-1\">Delete</button>\r\n                                  </td>\r\n                              </tr>\r\n                          </tbody>\r\n                      </table>\r\n                  </div>\r\n              </div>\r\n              <nav aria-label=\"Page navigation\">\r\n                  <ul class=\"pagination justify-content-center mt-4\">\r\n                      <li class=\"page-item\" v-for=\"pageNumber in totalPages\" :key=\"pageNumber\">\r\n                          <button class=\"page-link\" @click=\"goToPage(pageNumber)\">{{ pageNumber }}</button>\r\n                      </li>\r\n                  </ul>\r\n              </nav>\r\n          </div>\r\n      </layout-div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport LayoutDiv from '../LayoutDiv.vue';\r\nimport Swal from 'sweetalert2';\r\nimport SideBar from './SideBar';\r\n\r\nexport default {\r\n    name: 'LivreList',\r\n    components: {\r\n      LayoutDiv,\r\n      SideBar,\r\n    },\r\n    data() {\r\n        return {\r\n            projects:[],\r\n            currentPage: 1,\r\n            itemsPerPage: 4, // Nombre d'éléments par page\r\n            totalPages: 0,\r\n        };\r\n    },\r\n    created() {\r\n        this.fetchProjectList();\r\n    },\r\n    methods: {\r\n        fetchProjectList() {\r\n            const formData = new FormData();\r\n            if(sessionStorage.getItem('role') != null) {\r\n                formData.append('role', sessionStorage.getItem('role'));\r\n            }\r\n            const offset = (this.currentPage - 1) * this.itemsPerPage;\r\n            axios.post(`Biblio/tocrudlivre.do?offset=${offset}`, formData)\r\n                .then(response => {\r\n                    if(Object.keys(response.data).length === 0 ){\r\n                        this.$router.push('/');\r\n                    }\r\n                    this.projects = response.data.o;\r\n                    this.totalPages = Math.ceil(response.data.pages / this.itemsPerPage);\r\n                    for (let i = 0; i < this.projects.length; i++) {\r\n                        let obj = this.projects[i];\r\n                        for (let key in obj) {\r\n                            let value = obj[key];\r\n                            if (typeof value === 'object' && value !== null) {\r\n                                obj[key] = obj[key].label;\r\n                            }\r\n                        }\r\n                    }\r\n                    return response;\r\n                })\r\n                .catch(error => {\r\n                    return error;\r\n                });\r\n        },\r\n        goToPage(pageNumber) {\r\n            console.log('goToPage called with pageNumber:', pageNumber);\r\n            this.currentPage = pageNumber;\r\n            this.fetchProjectList();\r\n        },\r\n        handleDelete(id) {\r\n            Swal.fire({\r\n                title: 'Are you sure?',\r\n                text: \"You won't be able to revert this!\",\r\n                icon: 'warning',\r\n                showCancelButton: true,\r\n                confirmButtonColor: '#3085d6',\r\n                cancelButtonColor: '#d33',\r\n                confirmButtonText: 'Yes, delete it!'\r\n            }).then((result) => {\r\n                if (result.isConfirmed) {\r\n                    const formData = new FormData();\r\n                    if(sessionStorage.getItem('role') != null) {\r\n                        formData.append('role', sessionStorage.getItem('role'));\r\n                    }\r\n                    axios.post(`/Biblio/deletelivre.do?id=${id}`, formData)\r\n                        .then(response => {\r\n                            if(Object.keys(response.data).length === 0 ){\r\n                                this.$router.push('/');\r\n                            }\r\n                            Swal.fire({\r\n                                icon: 'success',\r\n                                title: 'Project deleted successfully!',\r\n                                showConfirmButton: false,\r\n                                timer: 1500\r\n                            });\r\n                            this.fetchProjectList();\r\n                            return response;\r\n                        })\r\n                        .catch(error => {\r\n                            Swal.fire({\r\n                                icon: 'error',\r\n                                title: 'An Error Occured!',\r\n                                showConfirmButton: false,\r\n                                timer: 1500\r\n                            });\r\n                            return error;\r\n                        });\r\n                }\r\n            });\r\n        }\r\n    }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.card {\r\n    border-radius: 15px;\r\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.card-header {\r\n    border-radius: 15px 15px 0 0;\r\n}\r\n\r\n.card-body {\r\n    border-radius: 0 0 15px 15px;\r\n}\r\n\r\n.btn-outline-primary {\r\n    color: #6c5ce7;\r\n    border-color: #6c5ce7;\r\n}\r\n\r\n.btn-outline-primary:hover {\r\n    background-color: #6c5ce7;\r\n    color: #fff;\r\n}\r\n\r\n.btn-outline-danger {\r\n    color: #ff4757;\r\n    border-color: #ff4757;\r\n}\r\n\r\n.btn-outline-danger:hover {\r\n    background-color: #ff4757;\r\n    color: #fff;\r\n}\r\n\r\n.pagination {\r\n    margin-top: 20px;\r\n}\r\n\r\n.sidebar {\r\n    position: fixed;\r\n    width: 250px;\r\n    height: 100%;\r\n    background-color: #f9eded;\r\n    padding: 30px 0;\r\n    border-right: 1px solid #e0e4e8;\r\n}\r\n\r\n.sidebar h2 {\r\n    color: #ff6b6b;\r\n    text-transform: uppercase;\r\n    text-align: center;\r\n    margin-bottom: 30px;\r\n}\r\n\r\n.sidebar ul li {\r\n    padding: 15px;\r\n    border-bottom: 1px solid #e0e4e8;\r\n    text-transform: capitalize; /* Met la première lettre en majuscule */\r\n}\r\n\r\n.sidebar ul li a {\r\n    color: #717171;\r\n    display: block;\r\n    text-transform: lowercase; /* Met le reste du texte en minuscules */\r\n}\r\n\r\n.sidebar ul li:hover {\r\n    background-color: #f1e6e6;\r\n}\r\n\r\n.sidebar ul li:hover a {\r\n    color: #ff6b6b;\r\n}\r\n\r\n.container {\r\n    margin-left: 250px;\r\n    margin-top: 20px;\r\n}\r\n\r\n@media (max-width: 768px) {\r\n    .sidebar {\r\n        width: 100%;\r\n        position: static;\r\n        border-right: none;\r\n    }\r\n\r\n    .container {\r\n        margin-left: 0;\r\n    }\r\n}\r\n</style>\r\n"],"mappings":";AAqDA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,SAAQ,MAAO,kBAAkB;AACxC,OAAOC,IAAG,MAAO,aAAa;AAC9B,OAAOC,OAAM,MAAO,WAAW;AAE/B,eAAe;EACXC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IACVJ,SAAS;IACTE;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,QAAQ,EAAC,EAAE;MACXC,WAAW,EAAE,CAAC;MACdC,YAAY,EAAE,CAAC;MAAE;MACjBC,UAAU,EAAE;IAChB,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,gBAAgB,CAAC,CAAC;EAC3B,CAAC;EACDC,OAAO,EAAE;IACLD,gBAAgBA,CAAA,EAAG;MACf,MAAME,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;MAC/B,IAAGC,cAAc,CAACC,OAAO,CAAC,MAAM,KAAK,IAAI,EAAE;QACvCH,QAAQ,CAACI,MAAM,CAAC,MAAM,EAAEF,cAAc,CAACC,OAAO,CAAC,MAAM,CAAC,CAAC;MAC3D;MACA,MAAME,MAAK,GAAI,CAAC,IAAI,CAACX,WAAU,GAAI,CAAC,IAAI,IAAI,CAACC,YAAY;MACzDT,KAAK,CAACoB,IAAI,CAAE,gCAA+BD,MAAO,EAAC,EAAEL,QAAQ,EACxDO,IAAI,CAACC,QAAO,IAAK;QACd,IAAGC,MAAM,CAACC,IAAI,CAACF,QAAQ,CAAChB,IAAI,CAAC,CAACmB,MAAK,KAAM,GAAG;UACxC,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,GAAG,CAAC;QAC1B;QACA,IAAI,CAACpB,QAAO,GAAIe,QAAQ,CAAChB,IAAI,CAACsB,CAAC;QAC/B,IAAI,CAAClB,UAAS,GAAImB,IAAI,CAACC,IAAI,CAACR,QAAQ,CAAChB,IAAI,CAACyB,KAAI,GAAI,IAAI,CAACtB,YAAY,CAAC;QACpE,KAAK,IAAIuB,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAI,IAAI,CAACzB,QAAQ,CAACkB,MAAM,EAAEO,CAAC,EAAE,EAAE;UAC3C,IAAIC,GAAE,GAAI,IAAI,CAAC1B,QAAQ,CAACyB,CAAC,CAAC;UAC1B,KAAK,IAAIE,GAAE,IAAKD,GAAG,EAAE;YACjB,IAAIE,KAAI,GAAIF,GAAG,CAACC,GAAG,CAAC;YACpB,IAAI,OAAOC,KAAI,KAAM,QAAO,IAAKA,KAAI,KAAM,IAAI,EAAE;cAC7CF,GAAG,CAACC,GAAG,IAAID,GAAG,CAACC,GAAG,CAAC,CAACE,KAAK;YAC7B;UACJ;QACJ;QACA,OAAOd,QAAQ;MACnB,CAAC,EACAe,KAAK,CAACC,KAAI,IAAK;QACZ,OAAOA,KAAK;MAChB,CAAC,CAAC;IACV,CAAC;IACDC,QAAQA,CAACC,UAAU,EAAE;MACjBC,OAAO,CAACC,GAAG,CAAC,kCAAkC,EAAEF,UAAU,CAAC;MAC3D,IAAI,CAAChC,WAAU,GAAIgC,UAAU;MAC7B,IAAI,CAAC5B,gBAA