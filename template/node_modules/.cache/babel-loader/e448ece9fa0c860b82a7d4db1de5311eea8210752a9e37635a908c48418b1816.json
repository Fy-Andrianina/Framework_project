able>\r\n                  </div>\r\n              </div>\r\n          </div>\r\n      </layout-div>\r\n  </template>\r\n   <script>\r\n import axios from 'axios';\r\n import LayoutDiv from '../LayoutDiv.vue';\r\n import Swal from 'sweetalert2'\r\n   \r\n  export default {\r\n    name: 'OlonaList',\r\n    components: {\r\n      LayoutDiv,\r\n    },\r\n    data() {\r\n      return {\r\n        projects:[]\r\n      };\r\n    },\r\n    created() {\r\n      this.fetchProjectList();\r\n    },\r\n    methods: {\r\n      fetchProjectList() {\r\n        axios.get('/api/projects')\r\n          .then(response => {\r\n              this.projects = response.data;\r\n              return response\r\n          })\r\n          .catch(error => {\r\n            return error\r\n          });\r\n      },\r\n      handleDelete(id){\r\n                  Swal.fire({\r\n              title: 'Are you sure?',\r\n              text: \"You won't be able to revert this!\",\r\n              icon: 'warning',\r\n              showCancelButton: true,\r\n              confirmButtonColor: '#3085d6',\r\n              cancelButtonColor: '#d33',\r\n              confirmButtonText: 'Yes, delete it!'\r\n            }).then((result) => {\r\n              if (result.isConfirmed) {\r\n                  axios.delete(`/api/projects/${id}`)\r\n                  .then( response => {\r\n                      Swal.fire({\r\n                          icon: 'success',\r\n                          title: 'Project deleted successfully!',\r\n                          showConfirmButton: false,\r\n                          timer: 1500\r\n                      })\r\n                      this.fetchProjectList();\r\n                      return response\r\n                  })\r\n                  .catch(error => {\r\n                      Swal.fire({\r\n                           icon: 'error',\r\n                          title: 'An Error Occured!',\r\n                          showConfirmButton: false,\r\n                          timer: 1500\r\n                      })\r\n                      return error\r\n                  });\r\n              }\r\n            })\r\n      }\r\n    },\r\n  };\r\n  </script>"],"mappings":"AA+CC,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,SAAQ,MAAO,kBAAkB;AACxC,OAAOC,IAAG,MAAO,aAAY;AAE5B,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAC;IACX,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,gBAAgB,CAAC,CAAC;EACzB,CAAC;EACDC,OAAO,EAAE;IACPD,gBAAgBA,CAAA,EAAG;MACjBR,KAAK,CAACU,GAAG,CAAC,eAAe,EACtBC,IAAI,CAACC,QAAO,IAAK;QACd,IAAI,CAACN,QAAO,GAAIM,QAAQ,CAACP,IAAI;QAC7B,OAAOO,QAAO;MAClB,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;QACd,OAAOA,KAAI;MACb,CAAC,CAAC;IACN,CAAC;IACDC,YAAYA,CAACC,EAAE,EAAC;MACJd,IAAI,CAACe,IAAI,CAAC;QACdC,KAAK,EAAE,eAAe;QACtBC,IAAI,EAAE,mCAAmC;QACzCC,IAAI,EAAE,SAAS;QACfC,gBAAgB,EAAE,IAAI;QACtBC,kBAAkB,EAAE,SAAS;QAC7BC,iBAAiB,EAAE,MAAM;QACzBC,iBAAiB,EAAE;MACrB,CAAC,CAAC,CAACb,IAAI,CAAEc,MAAM,IAAK;QAClB,IAAIA,MAAM,CAACC,WAAW,EAAE;UACpB1B,KAAK,CAAC2B,MAAM,CAAE,iBAAgBX,EAAG,EAAC,EACjCL,IAAI,CAAEC,QAAO,IAAK;YACfV,IAAI,CAACe,IAAI,CAAC;cACNG,IAAI,EAAE,SAAS;cACfF,KAAK,EAAE,+BAA+B;cACtCU,iBAAiB,EAAE,KAAK;cACxBC,KAAK,EAAE;YACX,CAAC;YACD,IAAI,CAACrB,gBAAgB,CAAC,CAAC;YACvB,OAAOI,QAAO;UAClB,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;YACZZ,IAAI,CAACe,IAAI,CAAC;cACLG,IAAI,EAAE,OAAO;cACdF,KAAK,EAAE,mBAAmB;cAC1BU,iBAAiB,EAAE,KAAK;cACxBC,KAAK,EAAE;YACX,CAAC;YACD,OAAOf,KAAI;UACf,CAAC,CAAC;QACN;MACF,CAAC;IACP;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}